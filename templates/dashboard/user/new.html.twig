{% extends 'dashboard/dashboardbase.html.twig' %}

{% block title %}Créer un utilisateur{% endblock %}

{% block body %}
    <h1>Créer un utilisateur</h1>
    
    {{ form_start(form) }}
        {{ form_row(form.email) }}
        {{ form_row(form.password) }}
        {{ form_row(form.firstName) }}
        {{ form_row(form.lastName) }}
        <button class="btn btn-success">{{ form_row(form.save) }}</button>
    {{ form_end(form) }}
{% endblock %}
